<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.spring.beteran.suggest" >
	<select id="movieIdList" resultType="movie">
		SELECT MOVIEID
		FROM MOVIE
	</select>
	
	
	<select id="rateListByMovieId" resultType="movieRate">
		SELECT MOVIEID, USERID, MOVIERATE
		FROM MOVIERATE
	</select>
	
 	
</mapper>  





















